<h3>New Skill</h3>

<%= form_for @skill, url: admin_skills_path do |f| %>
  <div class="form-group">
    <label>Skill</label>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <label>Level</label>
    <%= f.text_area :rating, class: 'form-control' %>
  </div>
    <div class="form-group">
    <%= f.submit "New Skill", class: 'btn btn-primary' %>
  </div>
<% end %>